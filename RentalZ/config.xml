<?xml version='1.0' encoding='utf-8'?>
<widget id="vn.edu.greenwich.rentalz" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>RentalZ</name>
    <description>
        A sample Apache Cordova RentalZ.
    </description>
    <author email="toantdgcs18873@fpt.edu.vn" href="https://greenwich.edu.vn/">
         Toan Tran
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
